&lt;script&gt;
    function toggleForm(id) {
        var form = document.querySelector(&#039;#&#039;+id);
        form.style.display = (form.style.display === &#039;none&#039;) ? &#039;block&#039; : &#039;none&#039;;
    }
    function enableButtonreply(id) {
        var input = document.getElementById(&#039;commentInput&#039;+id);
        var button = document.getElementById(&#039;submitButton&#039;+id);
        button.disabled = (input.value.trim() === &#039;&#039;);
    }
    function submitCommentReply(id) {
        // Dapatkan nilai dari input komentar
        var commentText = document.getElementById(&#039;commentInput&#039; + id).value;

        // Buat elemen div untuk menampilkan komentar
        var commentDiv = document.createElement(&#039;div&#039;);
        commentDiv.classList.add(&#039;d-flex&#039;, &#039;flex-row&#039;, &#039;p-3&#039;);

        // Tambahkan gambar dan informasi komentar ke dalam div
        commentDiv.innerHTML = `
            &lt;img src=&quot;https://i.imgur.com/BWz4Ga9.jpeg&quot; width=&quot;40&quot; height=&quot;40&quot; class=&quot;rounded-circle mr-3&quot;&gt;
            &lt;div class=&quot;w-100&quot;&gt;
                &lt;div class=&quot;d-flex justify-content-between align-items-center&quot;&gt;
                    &lt;div class=&quot;d-flex flex-row align-items-center&quot;&gt;
                        &lt;span class=&quot;mr-2 ms-1 fw-bold&quot;&gt;Anda&lt;/span&gt;
                        &lt;small class=&quot;c-badge ms-2 bg-warning&quot;&gt;Moderasi&lt;/small&gt;
                    &lt;/div&gt;
                &lt;/div&gt;
                &lt;p class=&quot;text-justify comment-text mb-0 ms-1&quot;&gt;${commentText}&lt;/p&gt;
            &lt;/div&gt;
        `;

        // Tampilkan informasi bahwa komentar sedang dimoderasi
        var moderationInfo = document.createElement(&#039;div&#039;);
        moderationInfo.innerHTML = &#039;&#039;;
        moderationInfo.classList.add(&#039;text-muted&#039;, &#039;ms-1&#039;);
        commentDiv.appendChild(moderationInfo);

        // Tambahkan elemen komentar ke dalam bagian komentar
        document.getElementById(&#039;commentReplySection&#039;+id).appendChild(commentDiv);

        // Nonaktifkan tombol setelah mengirim
        document.getElementById(&#039;submitButton&#039; + id).setAttribute(&#039;disabled&#039;, &#039;true&#039;);

        // Kosongkan input komentar setelah mengirim
        document.getElementById(&#039;commentInput&#039; + id).value = &#039;&#039;;
    }
&lt;/script&gt;
          &lt;script&gt;
    // Fungsi untuk menangani penambahan ketika tombol love diklik
function tambahLove(elementId) {
    // Dapatkan elemen span dengan ID likeCount
    var likeCountElement = $(&#039;#likeCount&#039; + elementId);
    var likeElement = $(&#039;#like&#039; + elementId);

    // Dapatkan nilai saat ini dan tambahkan satu
    var currentCount = parseInt(likeCountElement.text());
    var newCount = currentCount + 1;

    // Perbarui teks pada elemen
    likeCountElement.text(newCount);

    // Ganti fungsi onclick menjadi fungsi kurangLove
    $(&#039;#like&#039; + elementId).attr(&#039;onclick&#039;, &quot;kurangLove(&#039;&quot; + elementId + &quot;&#039;)&quot;);

    // Mengubah class dari &quot;unwish&quot; menjadi &quot;wish&quot;
    likeElement.removeClass(&quot;unwish&quot;).addClass(&quot;wish&quot;);
}

// Fungsi untuk menangani pengurangan ketika tombol love diklik
function kurangLove(elementId) {
    // Dapatkan elemen span dengan ID likeCount
    var likeCountElement = $(&#039;#likeCount&#039; + elementId);
    var likeElement = $(&#039;#like&#039; + elementId);

    // Dapatkan nilai saat ini dan kurangkan satu jika nilainya lebih besar dari nol
    var currentCount = parseInt(likeCountElement.text());
    var newCount = currentCount &gt; 0 ? currentCount - 1 : 0;

    // Perbarui teks pada elemen
    likeCountElement.text(newCount);

    // Ganti fungsi onclick menjadi fungsi tambahLove
    $(&#039;#like&#039; + elementId).attr(&#039;onclick&#039;, &quot;tambahLove(&#039;&quot; + elementId + &quot;&#039;)&quot;);

    // Mengubah class dari &quot;wish&quot; menjadi &quot;unwish&quot;
    likeElement.removeClass(&quot;wish&quot;).addClass(&quot;unwish&quot;);
}

