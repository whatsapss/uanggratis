&lt;script&gt;
  function enableButton() {
    // Dapatkan nilai dari input komentar
    var commentText = document.getElementById(&#039;commentInput&#039;).value;

    // Dapatkan tombol Kirim
    var submitButton = document.getElementById(&#039;submitButton&#039;);

    // Aktifkan tombol jika input komentar tidak kosong, nonaktifkan sebaliknya
    if (commentText.trim() !== &#039;&#039;) {
      submitButton.removeAttribute(&#039;disabled&#039;);
    } else {
      submitButton.setAttribute(&#039;disabled&#039;, &#039;true&#039;);
    }
  }

  function submitComment() {
    // Dapatkan nilai dari input komentar
    var commentText = document.getElementById(&#039;commentInput&#039;).value;

    // Buat elemen div untuk menampilkan komentar
    var commentDiv = document.createElement(&#039;div&#039;);
    commentDiv.classList.add(&#039;d-flex&#039;, &#039;flex-row&#039;, &#039;p-3&#039;);

    // Tambahkan gambar dan informasi komentar ke dalam div
    commentDiv.innerHTML = `
      &lt;img src=&quot;https://i.imgur.com/BWz4Ga9.jpeg&quot; width=&quot;40&quot; height=&quot;40&quot; class=&quot;rounded-circle mr-3&quot;&gt;
      &lt;div class=&quot;w-100&quot;&gt;
        &lt;div class=&quot;d-flex justify-content-between align-items-center&quot;&gt;
          &lt;div class=&quot;d-flex flex-row align-items-center&quot;&gt;
            &lt;span class=&quot;mr-2 ms-1 fw-bold&quot;&gt;Anda&lt;/span&gt;
            &lt;small class=&quot;c-badge ms-2 bg-warning&quot;&gt;Moderasi&lt;/small&gt;
          &lt;/div&gt;
          &lt;small&gt;Beberapa waktu yang lalu&lt;/small&gt;
        &lt;/div&gt;

        &lt;p class=&quot;text-justify comment-text mb-0 ms-1&quot;&gt;${commentText}&lt;/p&gt;
      &lt;/div&gt;
    `;

    // Tampilkan informasi bahwa komentar sedang dimoderasi
    var moderationInfo = document.createElement(&#039;div&#039;);
    moderationInfo.innerHTML = &#039;&#039;;
    moderationInfo.classList.add(&#039;text-muted&#039;, &#039;ms-1&#039;);
    commentDiv.appendChild(moderationInfo);

    // Tambahkan elemen komentar ke dalam bagian komentar
    document.getElementById(&#039;commentSection&#039;).appendChild(commentDiv);

    // Nonaktifkan tombol setelah mengirim
    document.getElementById(&#039;submitButton&#039;).setAttribute(&#039;disabled&#039;, &#039;true&#039;);

    // Kosongkan input komentar setelah mengirim
    document.getElementById(&#039;commentInput&#039;).value = &#039;&#039;;
  }
&lt;/script&gt;
